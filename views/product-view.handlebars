    <section id="main-content">
      <section class="wrapper">
        <div class="row">
           <div class="col-lg-10">

            <h3 class="page-header"><i class="fa fa-files-o"></i>PRODUCTS</h3>
          
          </div>
          <div class="col-lg-2">
             <a class="btn btn-primary page-header" href="/product/product-add"> ADD PRODUCTS</a>
          </div>
        </div>
        <!-- page start-->
        <div class="row">
          <div class="col-lg-12">
            <section class="panel">
              <header class="panel-heading">
               Manage Product
              </header>

              <table class="table table-striped table-advance table-hover tables">
                <thead>
                  <tr>
                    <th>#</th>
                    <th>Product Name</th>
                    <th>Product Type</th>
                    <th>Location</th>
                    <th>Period</th>
                    <th><i class="icon_cogs"></i> Action</th>
                  </tr>
                  </thead>
                  <tbody>
                   
                    {{#each productList  }}

                  
                  <tr>
                    <td>{{math @index "+" 1}}</td>
                    <td>{{name}}</td>
                    <td>{{type}}</td>
                    <td>{{ads_location}}</td>
                    <td>{{ads_period}}</td>
                    
                    <td>
                      <div class="btn-group">
                        <a class="btn btn-primary" href="/product/{{_id}}/file-view/"><i class="icon_document_alt"></i></a>
                       {{!--  <a class="btn btn-primary" href="/product/{{_id}}/product-assign-to-product"><i class="icon_desktop"></i></a> --}}
                        <a class="btn btn-success" href="/product/{{_id}}/update-view"><i class="icon_pencil-edit"></i></a>
                        <form style="display: flex;padding-bottom: 1px;" method="post" action="/product/{{_id}}/product-delete">
                        <button class="btn btn-danger" type="submit"><i class="icon_close_alt2"></i></button>
                        </form>
                      </div>
                    </td>
                  </tr>
                 {{/each}}
                </tbody>
              </table>
            </section>
          </div>
        </div>
        <!-- page end-->
      </section>
    </section>