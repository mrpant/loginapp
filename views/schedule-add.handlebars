 <section id="main-content">
      <section class="wrapper">
        <div class="row">
          <div class="col-lg-10">

            <h3 class="page-header"><i class="fa fa-files-o"></i>SCHEDULER</h3>
           
          </div>
          <div class="col-lg-2">
               <a class="btn btn-primary page-header" href="/scheduler"> VIEW SCHEDULER</a>
          </div>
        </div>
        <!-- Form validations -->
        <div class="row">
          <div class="col-lg-12">
            <section class="panel">
              <header class="panel-heading">
               Manage Schedule
              </header>
              <div class="panel-body">
                <div class="form">
                  <form class="form-validate form-horizontal" id="feedback_form" method="post" action="/scheduler/create">
                   {{#if schedulerDetails}}
                      <input type="hidden" name="schedulerId" value="{{schedulerDetails._id}}" />
                    {{/if}}
                    <div class="form-group ">
                      <label for="cname" class="control-label col-lg-2">Date <span class="required">*</span></label>
                      <div class="col-lg-10">
                        <input class="form-control" id="scheduleDate" name="scheduleDate" type="date" required
                        {{#if schedulerDetails}}
                         value="{{schedulerDetails.scheduleDate}}" 
                         {{/if}}
                         />
                      </div>
                    </div>
  
              
                     <div class="form-group ">
                      <label for="ccomment" class="control-label col-lg-2">Device <span class="required">*</span></label>
                      <div class="col-lg-10">
                           <select class="form-control" name="deviceId[]" multiple >
                          <option value="">Select Device</option>
                           {{#each deviceList}}

                           
                               <option value="{{_id}}" >{{deviceName}}</option>
                             
                        
                           {{/each}}
                        </select>
                      </div>
                     </div>
                    <div class="form-group">
                      <div class="col-lg-offset-2 col-lg-10">
                        <button class="btn btn-primary" type="submit">Save</button>
                        <button class="btn btn-default" type="button">Cancel</button>
                      </div>
                    </div>
                  </form>
                </div>

              </div>
            </section>
          </div>
        </div>
        
        <!-- page end-->
      </section>
    </section>