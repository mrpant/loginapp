    <section id="main-content">
      <section class="wrapper">
        <div class="row">
           <div class="col-lg-10">

            <h3 class="page-header"><i class="fa fa-files-o"></i>SCHEDULERS</h3>
           
          </div>
          <div class="col-lg-2">
             <a class="btn btn-primary page-header" href="/scheduler/scheduler-add"> ADD SCHEDULER</a>
          </div>
        </div>
        <!-- page start-->
        <div class="row">
          <div class="col-lg-12">
            <section class="panel">
              <header class="panel-heading">
                Scheduler
              </header>

              <table class="table table-striped table-advance table-hover tables">
                <thead>
                  <tr>
                    <th>#</th>
                    <th>Date</th>
                    <th>Device</th>
                    <th><i class="icon_cogs"></i> Action</th>
                  </tr>
                  </thead>
                  <tbody>
                    
                    {{#each schedulerList  }}
          
                  
                  <tr>
                    <td>{{math @index "+" 1}}</td>
                    <td>{{scheduleDate}}</td>
                    <td>{{deviceId.deviceName}}</td>
                  
                    
                    <td>
                      <div class="btn-group">
                        
                      {{!--  <a class="btn btn-primary" href="/scheduler/{{_id}}/file-view/"><i class="icon_document_alt"></i></a> --}}
                       {{!--  <a class="btn btn-primary" href="/product/{{_id}}/product-assign-to-product"><i class="icon_desktop"></i></a> --}}
                        {{!-- <a class="btn btn-success" href="/scheduler/{{_id}}/update-view"><i class="icon_pencil-edit"></i></a> --}}
                        <form style="display: flex;padding-bottom: 1px;" method="post" action="/scheduler/{{_id}}/delete">
                        <button class="btn btn-danger" type="submit"><i class="icon_close_alt2"></i></button>
                        </form>
                      </div>
                    </td>
                  </tr>
                 {{/each}}
                </tbody>
              </table>
            </section>
          </div>
        </div>
        <!-- page end-->
      </section>
    </section>